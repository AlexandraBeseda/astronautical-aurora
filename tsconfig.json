{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts",".astro/actions.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["vitest"], 
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
